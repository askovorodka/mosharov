$(function(){   $("#ring").popover({    placement: 'bottom',    html: 'true',  })  $(document).on('click', '#close', function (evente) {    $("#ring").popover('hide');  });  $("#close").click(function(event) {    $("#ring").popover('hide');  });  $(document).on('click', '#prepopover', function (evente) {    $("#ring").popover('hide');  });  $("#prepopover").click(function(event) {    $("#ring").popover('hide');  });  $('html').on('mouseup', function(e) {    if(!$(e.target).closest('.popover').length) {        $('.popover').each(function(){            $(this.previousSibling).popover('hide');        });    }});});$(function() {    $('[data-toggle="tooltip"]').tooltip({        placement: "right" ,        html: 'true'    });});