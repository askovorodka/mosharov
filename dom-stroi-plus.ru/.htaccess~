#php_value session.use_only_cookies 1
php_value session.use_trans_sid 0

RewriteEngine on
Options +FollowSymlinks
RewriteBase /

RewriteCond %{HTTP_HOST} ^www.dom-stroi-plus.ru$ [NC]
RewriteRule ^(.*)$ http://dom-stroi-plus.ru/$1 [R=301,L]

RewriteCond %{REQUEST_URI} ^/home$ [NC]
RewriteCond %{REQUEST_URI} ^/home/$ [NC]
RewriteRule ^(.*)$ http://dom-stroi-plus.ru/ [R=301,L]

RewriteCond 	%{REQUEST_FILENAME} !-f
RewriteRule     ^(.*)/video/(.*)$			/video/$2	[L]

RewriteCond 	%{REQUEST_FILENAME} !-f
RewriteRule     ^(.*)/insertfiles/(.*)$		/insertfiles/$2	[L]

RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d


RewriteRule ^(.*)$ /index.php [L,QSA]


